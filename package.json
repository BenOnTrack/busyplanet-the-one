		"carbon-components-svelte": "^0.82.1",
		"gh-pages": "^6.0.0",
		"svelte": "^3.53.1",
		"sveltestrap": "^5.11.2",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
